{
  "dart.flutterSdkPath": ".fvm/flutter_sdk",
  // Remove .fvm files from search
  "search.exclude": {
    "**/.fvm": true
  },
  // Remove from file watching
  "files.watcherExclude": {
    "**/.fvm": true
  },
  "dart.flutterTestAdditionalArgs": [
    "--flavor",
    "dev",
    "--dart-define-from-file",
    "configs/dart_define.dev.json",
  ],
}